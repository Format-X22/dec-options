{
  "name": "options-aggregator",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production nest build",
    "format": "prettier --write \"apps/**/*.(ts|tsx)\" \"libs/**/*.(ts|tsx)\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e:api": "jest --config ./apps/aggregation-api/test/jest-e2e.json --silent",
    "build:all": "nest build options-aggregator && nest build aggregation-api && nest build shared",
    "start:db": "docker compose up",
    "start:api": "nest start aggregation-api",
    "start:agg": "nest start options-aggregator",
    "start:prod:api": "node dist/apps/aggregation-api/apps/aggregation-api/src/main.js",
    "start:prod:agg": "node dist/apps/options-aggregator/apps/options-aggregator/src/main.js",
    "pm2:start:all:production": "npm run pm2:start:agg:production; npm run pm2:start:api:production;",
    "pm2:start:api:production": "NODE_ENV=production pm2 stop opex_api; pm2 delete opex_api; pm2 start ecosystem.config.js --only opex_api",
    "pm2:start:agg:production": "NODE_ENV=production pm2 stop opex_agg; pm2 delete opex_agg; pm2 start ecosystem.config.js --only opex_agg",
    "pm2:start:all:staging": "npm run pm2:start:agg:staging; npm run pm2:start:api:staging;",
    "pm2:start:api:staging": "NODE_ENV=production pm2 stop opex_api_staging; pm2 delete opex_api_staging; pm2 start ecosystem.config.js --only opex_api_staging",
    "pm2:start:agg:staging": "NODE_ENV=production pm2 stop opex_agg_staging; pm2 delete opex_agg_staging; pm2 start ecosystem.config.js --only opex_agg_staging"
  },
  "dependencies": {
    "@apollo/client": "^3.4.5",
    "@apollo/gateway": "^0.37.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/mongoose": "^8.0.0",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/schedule": "^1.0.1",
    "@types/graphql-fields": "^1.3.4",
    "@types/styled-components": "^5.1.9",
    "antd": "^4.15.4",
    "apollo-server-express": "^2.25.2",
    "bignumber.js": "^9.0.1",
    "ccxt": "^1.48.32",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.21.1",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-request": "^3.4.0",
    "graphql-tools": "^7.0.5",
    "greeks": "^1.0.0",
    "highcharts": "^9.1.2",
    "highcharts-react-official": "^3.0.0",
    "i": "^0.3.6",
    "implied-volatility": "^1.0.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongoose": "^5.12.5",
    "next": "^10.2.0",
    "npm": "^7.11.1",
    "pm2": "^4.5.6",
    "puppeteer": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sass": "^1.34.1",
    "sleep-promise": "^9.1.0",
    "styled-components": "^5.3.0",
    "swiper": "^6.7.0",
    "ts-morph": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.4.13",
    "@types/puppeteer": "^5.4.3",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.36.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@app/shared/(.*)": "<rootDir>/libs/shared/src/$1",
      "@app/shared": "<rootDir>/libs/shared/src"
    }
  }
}
